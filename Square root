import java .util.*;
public class Main{
    public static void main(String[] args) {
        Scanner d=new Scanner(System.in);
        int n=d.nextInt();
       int ans=1;
        for(int i=1;i<n/2;i++){
            if(i*i<=n){
               ans=i;
            }}
        System.out.println(ans);
        System.out.printf("%.3f",Math.sqrt(28));
    }
}
