import java.util.*;
public class Main{
    public static void main(String[]args){
        int a[][]={{2,3,4},
                {4,6,3},
                {2,6,3}};
        int transpose[][]=new int[a.length][a.length];
        for(int i=0;i<a.length;i++){
            for(int j=0;j<a.length;j++){
               transpose[j][i]=a[i][j];
            }}for(int i=0;i<a.length;i++){
            System.out.println(Arrays.toString(transpose[i]));}}}
