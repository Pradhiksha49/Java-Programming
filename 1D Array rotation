import java.util.*;
public class Main{
    public static void main(String[]args){
        int a[]={1,2,3,4};
        int d=1;
        for(int i=0;i<d;i++){
            int temp =a[0];
            for(int j=0;j<a.length-1;j++){
                a[j]=a[j+1];
            }a[a.length-1]=temp;
        }
        System.out.print(Arrays.toString(a));
        }}
