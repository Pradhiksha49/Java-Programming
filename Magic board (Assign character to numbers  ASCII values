import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner d=new Scanner(System.in);
        System.out.println("enter the number");
        int n = d.nextInt();
        int arr[]=new int [n];
        for(int i=0;i<n;i++) {
             arr[i]=d.nextInt();}
        for(int i=0;i<n;i++) {
            char ch = (char) arr[i];
            System.out.println(arr[i]+"-"+ch);
        }
    }
}
