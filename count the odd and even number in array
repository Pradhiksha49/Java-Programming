
 public class Main{
public static void main(String []args){
    int a[]={2,3,4,6,5,10,7,9,12};
    int evencount=0;
    int oddcount=0;
    for(int i=0;i<a.length;i++){
        if(a[i]%2==0){
            evencount++;
        }
        else{
            oddcount++;
        }
    }
    System.out.println("even:"+evencount);
    System.out.println("odd:"+oddcount);
}}
