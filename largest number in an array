public class Main{
    static int largest(int arr[]){
            int max=arr[0],min=arr[0];
            int n=arr.length;
             for(int i=0;i<n;i++){
                 if(arr[i]<min){
                     min=arr[i];
                 }
             }
            return min;
        }
    public static void main (String[]args){
             int a[]={15,72,21,30,42,50,35};
        int maximum=largest(a);
        System.out.println(maximum);
    }
}
