public class Main{
    public static void main (String[]args){
        int a[]={0,1,3,5};

     int n=a.length;
     for(int i=1;i<=a.length;i++) {
            boolean found=false;

        for(int j=0;j<n;j++){
           if( a[j]==i){
            found=true;
            break;
        }}
        if(!found){
            System.out.println(i);
        }
    }
}}
