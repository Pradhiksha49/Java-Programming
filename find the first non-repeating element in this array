public class Main {
    public static void main(String[] args) {
          int n[]={-1, 2, -1, 3, 0}; int result=0;
          for(int i=0;i<n.length;i++) {
              int count=0;
              for(int j=0;j<n.length;j++){
                  if(n[i]==n[j]){
                   count++;
                  }
              }
              if(count==1){
                  System.out.println(n[i]);
                  return;
             }
          }

          }
    }
