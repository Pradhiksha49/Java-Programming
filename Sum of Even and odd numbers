 public class Main{
    public static void main (String[] args){
        int n=8,i;
        int sum_even=0,sum_odd=0;
        for( i=0;i<n*2;i++) {
            if ((i & 1) == 0)
                sum_even += i;
             else
                sum_odd += i;
            }
            System.out.println(sum_even);
            System.out.println(sum_odd);
        }
    }
